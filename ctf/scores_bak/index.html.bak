                <td class="yellow-text">{{ entry.rank }}</td>
                <td class="green-text">{{ entry.name }}</td>
                <td class="cyan-text">{{ entry.points }}</td>
                <td class="red-text">{{ entry.bloods }}</td>
            </tr>
        {% endfor %}
    </table>
